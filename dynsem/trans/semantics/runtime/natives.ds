module semantics/runtime/natives

signature
  native operators
    error: String -> String
    str: AST -> String
    logdebug: String -> String
    int2string: Int -> String
    
  arrows
    halt-error(String) --> String
    log(String) --> String
    int2str(Int) --> String
    
rules

  halt-error(s) --> error(s)
  
  log(s) --> s
  where
    logdebug(s) => _
  
  int2str(i) --> int2string(i)
