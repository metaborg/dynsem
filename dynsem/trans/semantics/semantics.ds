module semantics/semantics

imports
  semantics/aterm
  semantics/terms
  semantics/module
  
signature
  sorts
    Eval
    
  constructors
    Eval : ATerm * Module -> Eval
    
  arrows
    Eval -init-> (T * Rules * Sorts * Constructors * Arrows)
    
rules
  
  Eval(t, spec) -init-> (prog, R, S, C, A)
  where
    convert(t) --> prog;
    spec :: S {}, C {}, A {}, R {} -load-> _ :: S, C, A, R

  