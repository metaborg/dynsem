module semantics/semantics

imports
  ds-signatures/ds-sig
  semantics/aterm
  semantics/spec-repr
  semantics/terms
  
signature
  sorts
    Eval
    Eval2
    
  constructors
    Eval : ATerm * Module -> Eval
    
  arrows
    Eval -init-> (T * Conses)
    
rules
  
